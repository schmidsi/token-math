{
  "name": "token-math",
  "description": "A small helper library to do precision safe calculations",
  "version": "0.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "parcel build src/index.ts && node dist/index.js",
    "test": "jest"
  },
  "engines": {
    "node": "^10.4.0"
  },
  "dependencies": {
    "@types/bn.js": "^4.11.2",
    "bn.js": "^4.11.8",
    "parcel-bundler": "^1.9.7"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
